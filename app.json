{
  "expo": {
    "name": "SigaMobile",
    "slug": "SigaMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SigaLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SigaLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jimmy03.SigaMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SigaLogo.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3840062f-ba82-4546-b610-87b20f6ffee0"
      }
    }
  }
}
